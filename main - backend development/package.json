{
  "name": "main",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "tsx src/seeders/using-prisma.ts"
  },
  "scripts": {
    "frontend:dev": "cd frontend && SET NODE_ENV=development & pnpm run dev",
    "frontend:build": "cd frontend && pnpm run build",
    "backend:dev": "SET NODE_ENV=development & npx tsx watch src/index.ts",
    "backend:build": "npx tsc",
    "backend:db:migrate:deploy": "npx prisma migrate deploy",
    "backend:db:migrate:dev": "npx prisma migrate dev",
    "backend:db:push": "npx prisma db push",
    "backend:db:push:force": "npx prisma db push --force-reset",
    "backend:db:seed": "npx prisma db seed",
    "backend:db:studio": "npx prisma studio",
    "build": "pnpm run frontend:build && pnpm run backend:build",
    "dev": "concurrently \"pnpm run frontend:dev\" \"pnpm run backend:dev\"",
    "start": "SET NODE_ENV=production & node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-zod-api": "^20.2.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.8",
    "zod-express-middleware": "^1.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.19.5",
    "@types/node": "^20.14.9",
    "concurrently": "^8.2.2",
    "prisma": "^5.16.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}